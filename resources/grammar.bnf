<root>      = (title | author | email)+
<meta-header> = '#+'
<meta-title>   = <meta-header> 'TITLE:' #"\s*"
<meta-author>  = <meta-header> 'AUTHOR:' #"\s*"
<meta-email>  = <meta-header> 'EMAIL:' #"\s*"
(* #+TITLE: content *)
title        = <meta-title>   #'.+'? <newline*>?
author       = <meta-author>  #'.+'? <newline*>?
email        = <meta-email>   #'.+'? <newline*>?
(*  *)
<tab>    = #'\t'
<space>    = #'\s'
<spaces>   = space+
<newline> = #'\n'